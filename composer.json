{
    "name": "dmyers/phoney",
    "description": "Free SMS notification channel for Laravel.",
    "keywords": ["laravel", "notifications", "sms gateway", "email2sms", "email to sms", "send free sms", "send sms"],
    "license": "MIT",
    "authors": [
        {
            "name": "Derek Myers",
            "email": "arcticpro@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "giggsey/libphonenumber-for-php": "^8.0@dev",
        "illuminate/console": "^8.0|^9.0|^10.0",
        "illuminate/contracts": "^8.0|^9.0|^10.0",
        "illuminate/notifications": "^8.0|^9.0|^10.0",
        "illuminate/support": "^8.0|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "Dmyers\\Phoney\\": "src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dmyers\\Phoney\\ServiceProvider"
            ],
            "aliases": {
                "Phoney": "Dmyers\\Phoney\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
